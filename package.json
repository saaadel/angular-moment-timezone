{
  "name": "angular-moment-timezone",
  "version": "1.7.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saaadel/angular-moment-timezone.git"
  },
  "author": "Andrew Spoda",
  "license": "MIT",
  "keywords": [
    "angular2",
    "angular4",
    "angular5",
    "momentjs",
    "moment",
    "moment-timezone"
  ],
  "bugs": {
    "url": "https://github.com/saaadel/angular-moment-timezone/issues"
  },
  "homepage": "https://github.com/saaadel/angular-moment-timezone#readme",
  "main": "index.js",
  "typings": "./index.d.ts",
  "files": [
    "index.d.ts",
    "index.js",
    "index.js.map",
    "index.metadata.json",
    "moment-timezone.module.d.ts",
    "moment-timezone.module.js",
    "moment-timezone.module.js.map",
    "moment-timezone.module.metadata.json",
    "tz.pipe.d.ts",
    "tz.pipe.js",
    "tz.pipe.js.map",
    "tz.pipe.metadata.json"
  ],
  "engines": {
    "node": ">=6.10.3",
    "npm": ">=3.10.10"
  },
  "scripts": {
    "prepublish": "npm run-script build && npm run-script prebuild:clean:out-tsc",
    "prebuild:clean:out-tsc": "del-cli -f out-tsc/",
    "prebuild:clean:root": "del-cli -f \"*.d.ts\" \"*.js.map\" \"*.metadata.json\" \"*.js\" \"!karma.conf.js\"",
    "prebuild": "npm run-script prebuild:clean:root && npm run-script prebuild:clean:out-tsc",
    "build": "ngc -p src/tsconfig.lib.json",
    "postbuild": "npm run-script postbuild:copy",
    "postbuild:copy:note": "# \"multiple sub-calls, because copyfiles does not support multiple exclude options\" 2>nul || exit 0",
    "postbuild:copy": "npm run-script postbuild:copy:d.ts && npm run-script postbuild:copy:js && npm run-script postbuild:copy:js.map && npm run-script postbuild:copy:metadata.json",
    "postbuild:copy:d.ts": "copyfiles -e \"out-tsc/lib/src/angular-moment-timezone/*.ngfactory.d.ts\" -f \"out-tsc/lib/src/angular-moment-timezone/*.d.ts\" ./",
    "postbuild:copy:js": "copyfiles -e \"out-tsc/lib/src/angular-moment-timezone/*.ngfactory.js\" -f \"out-tsc/lib/src/angular-moment-timezone/*.js\" ./",
    "postbuild:copy:js.map": "copyfiles -e \"out-tsc/lib/src/angular-moment-timezone/*.ngfactory.js.map\" -f \"out-tsc/lib/src/angular-moment-timezone/*.js.map\" ./",
    "postbuild:copy:metadata.json": "copyfiles -f \"out-tsc/lib/src/angular-moment-timezone/*.metadata.json\" ./",
    "ng": "ng",
    "ngc": "ngc",
    "start": "ng serve",
    "ng:build": "ng build",
    "test": "ng test --sr --browsers PhantomJS",
    "lint": "ng lint"
  },
  "dependencies": {
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "angular2-moment": "^1.7.0",
    "core-js": "^2.5.1",
    "moment": "^2.19.2",
    "moment-timezone": "^0.5.14",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "1.5.4",
    "@angular/compiler-cli": "^5.0.0",
    "@types/jasmine": "~2.8.2",
    "@types/moment-timezone": "^0.5.2",
    "@types/node": "~8.0.53",
    "codelyzer": "~4.0.1",
    "copyfiles": "~1.2.0",
    "del-cli": "^1.1.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "npm": ">=3.10.10",
    "protractor": "~5.2.0",
    "ts-node": "~3.3.0",
    "tslint": "~5.8.0",
    "typescript": "~2.6.1"
  }
}
